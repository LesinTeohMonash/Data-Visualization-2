      },
      "range": [0, 6.283185307179586],
      "zero": true
    }
  ],
  "legends": [
    {
      "labelColor": "#333d29",
      "titleColor": "#333d29",
      "fill": "color",
      "symbolType": "circle",
      "title": "Type of Milk",
      "encode": {
        "labels": {
          "name": "Type_of_Milk_legend_labels",
          "interactive": true,
          "update": {
            "opacity": [
              {
                "test": "(!length(data(\"Milk_Filter_store\")) || (Milk_Filter[\"Type of Milk\"] && indexof(Milk_Filter[\"Type of Milk\"], datum.value) >= 0))",
                "value": 1
              },
              {"value": 0.35}
            ]
          }
        },
        "symbols": {
          "name": "Type_of_Milk_legend_symbols",
          "interactive": true,
          "update": {
            "opacity": [
