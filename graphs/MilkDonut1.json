  "scales": [
    {
      "name": "color",
      "type": "ordinal",
      "domain": [
        "Almond milk",
        "Dairy milk",
        "Oat milk",
        "Rice milk",
        "Soy milk"
      ],
      "range": ["#ecf07b", "#e2e859", "#d7de4b", "#cad33d", "#bdc849"]
    },
    {
      "name": "concat_0_theta",
      "type": "linear",
      "domain": {
        "data": "data_0",
        "fields": [
          "Eutrophication from milks (g PO43-eq)_start",
          "Eutrophication from milks (g PO43-eq)_end"
        ]
      },
      "range": [0, 6.283185307179586],
      "zero": true
    },
    {
      "name": "concat_1_theta",
      "type": "linear",
      "domain": {
        "data": "data_1",
        "fields": [
          "GHG emissions of milks (kg CO2eq)_start",
          "GHG emissions of milks (kg CO2eq)_end"
        ]
      },
      "range": [0, 6.283185307179586],
      "zero": true
    },
    {
      "name": "concat_2_theta",
